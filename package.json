{
  "name": "debugging-talk",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:watch": "tsc-watch --onSuccess 'node ./bin/www' --onFailure 'echo Compile Failed' --compiler typescript/bin/tsc",
    "start:watch:debug": "tsc-watch --onSuccess 'node --inspect ./bin/www' --onFailure 'echo Compile Failed' --compiler typescript/bin/tsc",
    "build": "tsc"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "express": "~4.16.1",
    "faker": "^4.1.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/express": "~4.16.1",
    "@types/faker": "^4.1.5",
    "nodemon": "^1.19.1",
    "tsc-watch": "^2.4.0",
    "typescript": "^3.5.3"
  }
}